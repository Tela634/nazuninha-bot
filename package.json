{
  "name": "nazuninha-bot",
  "version": "1.1",
  "description": "O bot de whatsapp mais leve e funcional que voce ja viu",
  "main": "app.js",
  "scripts": {
    "start": "npm run start:silent --silent",
    "start:silent": "sh dados/src/.scripts/.start.sh",
    "start:add": "npm run start:add:silent --silent",
    "start:add:silent": "sh dados/src/.scripts/.addnumber.sh",
    "config": "npm run config:silent --silent",
    "update": "npm run update:silent --silent",
    "update:silent": "sh dados/src/.scripts/.update.sh",
    "config:install": "npm run config:install:silent --silent",
    "config:silent": "sh dados/src/.scripts/.config.sh",
    "config:install:silent": "sh dados/src/.scripts/.config.sh --install"
  },
  "repository": {
    "type": "git",
    "url": "github.com/hiudyy/nazuninha-bot"
  },
  "author": "Hiudy",
  "license": "ISC",
  "dependencies": {
    "baileys": "npm:@fizzxydev/baileys-pro@latest",
    "linkedom": "latest",
    "fluent-ffmpeg": "latest",
    "node-webpmux": "latest",
    "qrcode-terminal": "latest",
    "yt-search": "latest",
    "ruhend-scraper": "latest",
    "hercai": "latest",
    "swiftly": "latest"
  }
}
